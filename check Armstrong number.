  int a = 1634;
        System.out.println();
        String b = String.valueOf(a);
        long c = 0;
        for (int i = 0; i < b.length(); i++) {
            c += Math.pow(Integer.valueOf(b.split("")[i]), b.length());
        }
        System.out.println(a);
        System.out.println(c);
        System.out.println(String.valueOf(a).equals(String.valueOf(c)));
